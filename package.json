    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/vow": "^1.0.17",
    "@hapi/joi": "^17.1.0",
    "axios": "^0.21.1",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.5.0",
    "https": "^1.0.0",
    "husky": "^4.2.3",
    "joi": "^14.3.1",
    "mongoose": "^5.9.3",
    "mz": "^2.7.0",
    "nock": "^12.0.2",
    "nyc": "^15.0.0",
